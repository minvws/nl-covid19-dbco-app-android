<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/onboardingFragmentHelp">

    <action
        android:id="@+id/action_end_of_life"
        app:destination="@id/nav_end_of_life"
        app:popUpTo="@id/nav_main"
        app:popUpToInclusive="true" />

    <action
        android:id="@+id/action_app_update_required"
        app:destination="@id/nav_app_update_required"
        app:popUpTo="@id/nav_main"
        app:popUpToInclusive="true" />


    <fragment
        android:id="@+id/placeholderFragment"
        android:name="nl.rijksoverheid.dbco.debug.PlaceholderFragment"
        android:label="PlaceholderFragment"
        tools:layout="@layout/fragment_placeholder">
        <action
            android:id="@+id/toStylingFragment"
            app:destination="@id/stylingDebugFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/toAboutFragment"
            app:destination="@id/aboutFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/toMyContactsFragment"
            app:destination="@id/myContactsFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>

    <fragment
        android:id="@+id/stylingDebugFragment"
        android:name="nl.rijksoverheid.dbco.debug.StylingDebugFragment"
        android:label="StylingDebugFragment"
        tools:layout="@layout/fragment_styling" />

    <fragment
        android:id="@+id/aboutFragment"
        android:name="nl.rijksoverheid.dbco.about.AboutFragment"
        android:label="AboutFragment"
        tools:layout="@layout/fragment_list">
        <action
            android:id="@+id/toAboutDetailFragment"
            app:destination="@id/FAQDetailFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>
    <fragment
        android:id="@+id/FAQDetailFragment"
        android:name="nl.rijksoverheid.dbco.about.faq.FAQDetailFragment"
        android:label="FAQDetailFragment">
        <argument
            android:name="faqItemId"
            app:argType="nl.rijksoverheid.dbco.about.faq.FAQItemId" />
    </fragment>
    <fragment
        android:id="@+id/contactPickerPermission"
        tools:layout="@layout/fragment_list"
        android:name="nl.rijksoverheid.dbco.contacts.picker.ContactPickerPermissionFragment"
        android:label="ContactPickerPermissionFragment">
        <argument
            android:name="indexTask"
            android:defaultValue="@null"
            app:nullable="true"
            app:argType="nl.rijksoverheid.dbco.tasks.data.entity.Task" />
        <action
            android:id="@+id/toContactPicker"
            app:destination="@id/contactPickerSelectionFragment"
            app:popUpTo="@+id/myContactsFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />

        <action
            android:id="@+id/toContactDetails"
            app:destination="@id/contactDetailsInputFragment"
            app:popUpTo="@+id/myContactsFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>
    <fragment
        android:id="@+id/contactPickerSelectionFragment"
        android:name="nl.rijksoverheid.dbco.contacts.picker.ContactPickerSelectionFragment"
        tools:layout="@layout/fragment_contact_selection"
        android:label="ContactPickerSelectionFragment">
        <argument
            android:name="indexTask"
            android:defaultValue="@null"
            app:nullable="true"
            app:argType="nl.rijksoverheid.dbco.tasks.data.entity.Task" />
        <action
            android:id="@+id/toContactDetails"
            app:popUpTo="@id/myContactsFragment"
            app:destination="@id/contactDetailsInputFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
    </fragment>
    <fragment
        android:id="@+id/myContactsFragment"
        android:name="nl.rijksoverheid.dbco.contacts.mycontacts.MyContactsFragment"
        tools:layout="@layout/fragment_my_contacts"
        android:label="MyContactsFragment">
        <action
            android:id="@+id/toContactPickerPermission"
            app:destination="@id/contactPickerPermission"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popUpTo="@id/myContactsFragment"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/toContactPickerSelection"
            app:destination="@id/contactPickerSelectionFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popUpTo="@id/myContactsFragment"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />

        <action
            android:id="@+id/toFinalizeCheck"
            app:destination="@id/finalizeCheckFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_close_enter"
            app:popExitAnim="@anim/fragment_close_exit" />
        <action
            android:id="@+id/toContactDetails"
            app:destination="@id/contactDetailsInputFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/contactDetailsInputFragment"
        tools:layout="@layout/fragment_contact_input"
        android:name="nl.rijksoverheid.dbco.contacts.details.ContactDetailsInputFragment"
        android:label="ContactDetailsInputFragment">
        <argument
            android:name="indexTask"
            android:defaultValue="@null"
            app:nullable="true"
            app:argType="nl.rijksoverheid.dbco.tasks.data.entity.Task" />
        <argument
            android:name="selectedContact"
            android:defaultValue="@null"
            app:nullable="true"
            app:argType="nl.rijksoverheid.dbco.contacts.data.entity.LocalContact" />
    </fragment>

    <fragment
        android:id="@+id/onboardingFragmentHelp"
        android:name="nl.rijksoverheid.dbco.onboarding.OnboardingHelpFragment"
        android:label="OnboardingHelpFragment"
        tools:layout="@layout/fragment_onboarding_help">
        <action
            android:id="@+id/toFillCodeFragment"
            app:destination="@id/fillCodeFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/toMyContacts"
            app:destination="@id/myContactsFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/fillCodeFragment"
        android:name="nl.rijksoverheid.dbco.onboarding.FillCodeFragment"
        android:label="FillCodeFragment"
        tools:layout="@layout/fragment_fill_code">
        <action
            android:id="@+id/toOnboardingAddDataFragment"
            app:destination="@+id/onboardingAddDataFragment"
            app:popUpTo="@+id/onboardingFragmentHelp"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/onboardingAddDataFragment"
        android:name="nl.rijksoverheid.dbco.onboarding.OnboardingAddDataFragment"
        android:label="OnboardingAddDataFragment"
        tools:layout="@layout/fragment_onboarding_add_data">
        <action
            android:id="@+id/toMyContactsFragment"
            app:destination="@id/myContactsFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/finalizeCheckFragment"
        tools:layout="@layout/fragment_finalizing_check"
        android:name="nl.rijksoverheid.dbco.finalizing.FinalizeCheckFragment"
        android:label="FinalizeCheckFragment">
        <action
            android:id="@+id/toFinalizeSentFragment"
            app:destination="@id/finalizeSentFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            android:id="@+id/toContactDetailsInputFragment"
            app:destination="@id/contactDetailsInputFragment" />
    </fragment>

    <fragment
        android:id="@+id/finalizeSentFragment"
        android:name="nl.rijksoverheid.dbco.finalizing.FinalizeSentFragment"
        android:label="FinalizeSentFragment">
        <action
            android:id="@+id/toMyContactsFragment"
            app:destination="@id/myContactsFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>


    <fragment
        android:id="@+id/nav_end_of_life"
        android:name="nl.rijksoverheid.dbco.applifecycle.EndOfLifeFragment"
        tools:layout="@layout/fragment_end_of_life" />

    <fragment
        android:id="@+id/nav_app_update_required"
        android:name="nl.rijksoverheid.dbco.applifecycle.AppUpdateRequiredFragment"
        tools:layout="@layout/fragment_app_update_required">
        <argument
            android:name="app_store_package"
            app:argType="string"
            app:nullable="true" />
    </fragment>


</navigation>